{
  "name": "vitable",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vitable.umd.js",
  "module": "./dist/vitable.es.js",
  "exports": {
    ".": {
      "import": "./dist/vitable.es.js",
      "require": "./dist/vitable.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite build --watch && vue-tsc --emitDeclarationOnly",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "test:watch": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/test-utils": "^2.0.2",
    "jsdom": "^20.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitest": "^0.21.0",
    "vue-tsc": "^0.38.4"
  }
}
